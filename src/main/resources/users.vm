<html>
<head>
    <title>Users</title>
    <style>
        table, tr, td {
            border: 1px solid coral;
            padding: 10px;
            width: 100%;
        }
    </style>
</head>
<body>
<h1> Users </h1>
<table>
    <tr>
        <td>ID</td>
        <td>FIRST NAME</td>
        <td>LAST NAME</td>

    </tr>
    #foreach($user in $users)
        <tr>
            <td>$user.id</td>
            <td>$user.first_name</td>
            <td>$user.last_name</td>
        </tr>
    #end
</table>
</body>
</html>